{
  "name": "subdomain-test",
  "version": "0.0.1",
  "description": "SvelteKit application with Bun runtime",
  "main": "build/index.js",
  "scripts": {
    "start": "bun run start",
    "build": "bun run build",
    "dev": "bun run dev"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "railpack": {
    "runtime": "bun",
    "buildCommand": "bun run build",
    "startCommand": "bun run start",
    "port": 3000,
    "healthCheck": "/",
    "environment": {
      "NODE_ENV": "production"
    }
  }
}